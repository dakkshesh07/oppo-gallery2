<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/picture3d_editor_ic_ratio_1x1_normal" android:tint="@color/common_C12" android:alpha="0.15" />
    </item>
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/picture3d_editor_ic_ratio_1x1_normal" android:tint="@color/common_C12" android:alpha="0.2" />
    </item>
    <item android:state_selected="true">
        <bitmap android:src="@drawable/picture3d_editor_ic_ratio_1x1_normal" android:tint="@color/picture3d_editor_item_selected_filter_color" />
    </item>
    <item>
        <bitmap android:src="@drawable/picture3d_editor_ic_ratio_1x1_normal" android:alpha="1.0" />
    </item>
</selector>