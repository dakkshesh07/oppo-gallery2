<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <layer-list>
            <item>
                <bitmap android:src="@drawable/picture3d_editor_ic_mosaic_style_square_normal" />
            </item>
            <item>
                <bitmap android:src="@drawable/picture3d_editor_ic_mosaic_style_selected" />
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <bitmap android:src="@drawable/picture3d_editor_ic_mosaic_style_square_normal" />
            </item>
            <item>
                <bitmap android:src="@drawable/picture3d_editor_ic_mosaic_style_selected" />
            </item>
        </layer-list>
    </item>
    <item android:drawable="@drawable/picture3d_editor_ic_mosaic_style_square_normal" />
</selector>